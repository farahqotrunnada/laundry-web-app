{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma": "npx prisma",
    "email": "dotenv -e .env.development.local -e .env.development -- email dev --port 3001 --dir src/emails",
    "db:migrate": "dotenv -e .env.development.local -e .env.development -- npx prisma migrate dev",
    "db:generate": "dotenv -e .env.development.local -e .env.development -- npx prisma generate",
    "db:pull": "dotenv -e .env.development.local -e .env.development -- npx prisma db pull",
    "db:push": "dotenv -e .env.development.local -e .env.development -- npx prisma db push",
    "db:studio": "dotenv -e .env.development.local -e .env.development -- npx prisma studio"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@react-email/components": "^0.0.25",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "resend": "^4.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/passport-google-oauth20": "^2.0.16",
    "prisma": "^5.7.1",
    "react-email": "^3.0.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}
